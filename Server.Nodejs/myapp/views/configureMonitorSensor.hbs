<div id="select-parameters" style="color:#000045"><font size="4">
  <form action="/monitoring" method="post">
    <div id="allSensors">
      <label>Choose the Sensor (code-room-shed)</label>
      <select name="sensor">
        {{#each sensors as |val key|}}
          <option value="{{val}}">{{val}}</option>
        {{/each}}
      </select>
    </div>
    <br></br>
    <button type="submit">Submit</button>
  </form>
</font></div>
